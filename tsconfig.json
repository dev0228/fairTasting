{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "paths": {
      "@assets/*": [
        "./assets/*"
      ],
      "@fair/*": [
        "./src/*"
      ],
    }
  },
  "extends": "expo/tsconfig.base"
}
